{
    /* 
     * Requires the Rust Language Server (rust-analyzer) and Cortex-Debug extensions
     * https://marketplace.visualstudio.com/items?itemName=rust-lang.rust-analyzer
     * https://marketplace.visualstudio.com/items?itemName=marus25.cortex-debug
     */
    "version": "0.2.0",
    "configurations": [
        {
            /* Configuration for the {{ target }} MCU */
            "type": "cortex-debug",
            "request": "launch",
            "name": "Debug (OpenOCD)",
            "servertype": "openocd",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "Flash",
            "runToEntryPoint": "main",
            "executable": "target/{{ architecture_0 }}/debug/{{ core_0 }}",
            /* Run `cargo build --example itm` and uncomment this line to run itm example */
            // "executable": "./build/thumbv7em-none-eabihf/debug/examples/itm",
            "device": "{{ target }}",
            "configFiles": [
                "interface/{{ openocd_interface_0 }}",
                "target/{{ openocd_target_0 }}"
            ],
            "svdFile": "${workspaceRoot}/.vscode/{{ target }}.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 8000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    { "type": "console", "label": "ITM", "port": 0 }
                ]
            }
        }
    ]
}